{% trans_default_domain 'FOSUserBundle' %}

{% form_theme form 'TangaraCoreBundle:Form:fields.html.twig' %}

<form action="{{ path('tangara_user_change_password') }}" {{ form_enctype(form) }} method="POST" class="fos_user_change_password content-form" role="form">
    <div class="form-group">    
        {{ form_widget(form) }}
    </div>
    <button type="submit" class="btn btn-primary">{{ 'change_password.submit'|trans }} </button>
</form>
