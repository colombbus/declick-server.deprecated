{% trans_default_domain 'FOSUserBundle' %}

{% form_theme form 'TangaraCoreBundle:Form:fields.html.twig' %}

<form action="{{ path('tangara_user_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register content-form" role="form">
    <div class="form-group">
        {{ form_widget(form) }}
    </div>
    <button type="submit" class="btn btn-primary">{{ 'registration.submit'|trans }} </button>
</form>
