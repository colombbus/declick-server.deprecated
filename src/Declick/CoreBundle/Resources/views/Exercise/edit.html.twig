{% extends 'DeclickCoreBundle:Main:content.html.twig' %}
{% form_theme form 'DeclickCoreBundle:Form:fields.html.twig' %}

{% block title %}
    {{title|trans}}
{% endblock %}

{% block content %}
    <div class="row">
        <div class="col-md-12">
            <form action="" method="post" name="step" class="content-form">
                {{ form_rest(form) }}
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}

{% block script %}
    $(function(){
        $('#project_cancel').click(function(e){
            window.history.back();
        });
    });
{% endblock %}
