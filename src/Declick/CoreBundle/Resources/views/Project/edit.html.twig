{% extends 'DeclickCoreBundle:Main:content.html.twig' %}
{% form_theme form 'DeclickCoreBundle:Form:fields.html.twig' %}


{% block title %}
    {{title|trans}}
{% endblock %}

{% block content %}
    <div class="row">
        <div class="col-md-12">
            <form action="" method="post" name="project" class="content-form">
                {{ form_row(form.name) }}
                {{ form_row(form.published) }}
            {% if form.launcher is defined %}
                {{ form_row(form.launcher) }}
            {% endif %}
                {{ form_row(form.width) }}
                {{ form_row(form.height) }}
                {{ form_row(form.description) }}
                {{ form_row(form.instructions) }}
            {% if form.readonly is defined %}
                {{ form_row(form.readonly) }}
            {% endif %}
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}

{% block script %}
    $(function(){
        $('#project_cancel').click(function(e){
            window.history.back();
        });
    });
{% endblock %}    
